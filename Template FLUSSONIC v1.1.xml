<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>5.0</version>
    <date>2021-11-09T15:11:16Z</date>
    <groups>
        <group>
            <name>NOC_SERVERS</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Template FLUSSONIC  v1.1</template>
            <name>Template FLUSSONIC  v1.1</name>
            <description>- V1.1 Fixed bitrate item value from numeric(unsigned) to numeric(float)</description>
            <templates>
                <template>
                    <name>Template Module ICMP Ping</name>
                </template>
            </templates>
            <groups>
                <group>
                    <name>NOC_SERVERS</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>Server Info</name>
                </application>
                <application>
                    <name>Streams Info</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>Total Connected Clients</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>1.3.6.1.4.1.36342.1.2.1.0</snmp_oid>
                    <key>total.flussonic</key>
                    <units>client(s)</units>
                    <applications>
                        <application>
                            <name>Server Info</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Total Streams Number</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>1.3.6.1.4.1.36342.1.1.1.0</snmp_oid>
                    <key>total.streams</key>
                    <units>stream(s)</units>
                    <applications>
                        <application>
                            <name>Server Info</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Server Uptime</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.2.1.1.3.0</snmp_oid>
                    <key>uptime.flussonic</key>
                    <delay>5m</delay>
                    <units>s</units>
                    <applications>
                        <application>
                            <name>Server Info</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>MULTIPLIER</type>
                            <params>0.01</params>
                        </step>
                    </preprocessing>
                    <triggers>
                        <trigger>
                            <expression>{last()}&lt;10m</expression>
                            <name>{HOST.NAME} has been restarted (uptime &lt; 10m)</name>
                            <priority>WARNING</priority>
                            <manual_close>YES</manual_close>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Server Version</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.2.1.1.1.0</snmp_oid>
                    <key>version.flussonic</key>
                    <delay>1d</delay>
                    <trends>0</trends>
                    <value_type>CHAR</value_type>
                    <applications>
                        <application>
                            <name>Server Info</name>
                        </application>
                    </applications>
                </item>
            </items>
            <discovery_rules>
                <discovery_rule>
                    <name>Streams Discovery</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.36342.1.1.2.1.1,{#IFNAME},1.3.6.1.4.1.36342.1.1.2.1.2]</snmp_oid>
                    <key>stream.discovery</key>
                    <delay>5m</delay>
                    <item_prototypes>
                        <item_prototype>
                            <name>Stream {#IFNAME}:  Bitrate</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.4.1.36342.1.1.2.1.6.{#SNMPINDEX}</snmp_oid>
                            <key>flusstreams[bitrate.{#SNMPINDEX}]</key>
                            <value_type>FLOAT</value_type>
                            <units>Mbps</units>
                            <applications>
                                <application>
                                    <name>Streams Info</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <params>0.0009765625</params>
                                </step>
                            </preprocessing>
                        </item_prototype>
                        <item_prototype>
                            <name>Stream {#IFNAME}:  Client Watching Count</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.4.1.36342.1.1.2.1.3.{#SNMPINDEX}</snmp_oid>
                            <key>flusstreams[clientsc.{#SNMPINDEX}]</key>
                            <units>clientes</units>
                            <applications>
                                <application>
                                    <name>Streams Info</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Stream {#IFNAME}:  Retry Count</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.4.1.36342.1.1.2.1.4.{#SNMPINDEX}</snmp_oid>
                            <key>flusstreams[retryc.{#SNMPINDEX}]</key>
                            <delay>30s</delay>
                            <units>Retry(ies)</units>
                            <applications>
                                <application>
                                    <name>Streams Info</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Stream {#IFNAME}:  Status</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.4.1.36342.1.1.2.1.9.{#SNMPINDEX}</snmp_oid>
                            <key>flusstreams[status.{#SNMPINDEX}]</key>
                            <description>ROWSTATUS INTEGER FOR VALUE MAPPING {active(1), notInService(2), notReady(3), createAndGo(4), createAndWait(5), destroy(6) }</description>
                            <applications>
                                <application>
                                    <name>Streams Info</name>
                                </application>
                            </applications>
                            <valuemap>
                                <name>FLUSSONIC_STREAM_STATUS</name>
                            </valuemap>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last()}&gt;1</expression>
                                    <name>Stream {#IFNAME} is not RUNNING, check status</name>
                                    <priority>HIGH</priority>
                                    <manual_close>YES</manual_close>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>Stream {#IFNAME}:  Uptime</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>.1.3.6.1.4.1.36342.1.1.2.1.5.{#SNMPINDEX}</snmp_oid>
                            <key>flusstreams[streamup.{#SNMPINDEX}]</key>
                            <units>s</units>
                            <applications>
                                <application>
                                    <name>Streams Info</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <params>0.001</params>
                                </step>
                            </preprocessing>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last()}&lt;10m</expression>
                                    <name>Stream {#IFNAME} restarted, uptime &lt;10m</name>
                                    <priority>WARNING</priority>
                                    <manual_close>YES</manual_close>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
            </discovery_rules>
        </template>
    </templates>
    <value_maps>
        <value_map>
            <name>FLUSSONIC_STREAM_STATUS</name>
            <mappings>
                <mapping>
                    <value>1</value>
                    <newvalue>ONLINE</newvalue>
                </mapping>
                <mapping>
                    <value>2</value>
                    <newvalue>NOT_IN_SERVICE</newvalue>
                </mapping>
                <mapping>
                    <value>3</value>
                    <newvalue>NOT_READY</newvalue>
                </mapping>
                <mapping>
                    <value>4</value>
                    <newvalue>CREATE_AND_GO</newvalue>
                </mapping>
                <mapping>
                    <value>5</value>
                    <newvalue>CREATE_AND_WAIT</newvalue>
                </mapping>
                <mapping>
                    <value>6</value>
                    <newvalue>DESTROY</newvalue>
                </mapping>
            </mappings>
        </value_map>
    </value_maps>
</zabbix_export>
